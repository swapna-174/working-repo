
SELECT *FROM caboodle.BillingAccountFact
LEFT JOIN caboodle.ProcedureBridge ON BillingAccountFact.IcdProcedureComboKey = ProcedureBridge.ProcedureComboKey
LEFT JOIN caboodle.ProcedureDim ON ProcedureDurableKey =ProcedureDim.durablekey
LEFT JOIN caboodle.patientdim ON BillingAccountFact.patientdurablekey=patientdim.durablekey
LEFT JOIN caboodle.CoverageDim ON BillingAccountFact. FirstCoverageKey=CoverageDim.CoverageKey

where ProcedureDim.code IN (
 
 '58150',
 '58152',
 '58260')
