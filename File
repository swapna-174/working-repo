SELECT
HSP_ACCT_WQ_ITEMS.ITEM_ID
,HSP_ACCT_WQ_ITEMS.WORKQUEUE_ID
,HSP_ACCT_WQ_ITEMS.HSP_ACCOUNT_ID
,CAST(HSP_ACCT_WQ_ITEMS.ENTRY_DATE AS date) ENTRY_DATE
,CAST(HSP_ACCT_WQ_ITEMS.RELEASE_DATE AS date) RELEASE_DATE
,HSP_ACCT_WQ_ITEMS.DEFERRAL_DUE_DTTM
,CASE WHEN HSP_ACCT_WQ_ITEMS.TAB_STATUS_C=0 THEN 1 ELSE 0 END AS IS_ACTIVE
,ZC_TICKLER_REASON.NAME AS DEFERRAL_REASON
FROM HSP_ACCT_WQ_ITEMS

LEFT OUTER JOIN ZC_TICKLER_REASON
	ON ZC_TICKLER_REASON.TICKLER_REASON_C = HSP_ACCT_WQ_ITEMS.DEFERRAL_REASON_C
