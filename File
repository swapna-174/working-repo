USE [msdb]
GO

/****** Object:  ProxyAccount [svc_birptsol]    Script Date: 12/11/2024 1:47:40 PM ******/
EXEC msdb.dbo.sp_add_proxy @proxy_name=N'svc_birptsol',@credential_name=N'svc_birptsol', 
		@enabled=1
GO

EXEC msdb.dbo.sp_grant_proxy_to_subsystem @proxy_name=N'svc_birptsol', @subsystem_id=11
GO


